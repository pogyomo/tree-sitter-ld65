{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ld65",
      "camelcase": "LD65",
      "title": "ld65",
      "scope": "source.ld65",
      "file-types": [
        "ld65"
      ],
      "injection-regex": "^ld65$",
      "class-name": "TreeSitterLd65"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "ld65 grammer for tree-sitter",
    "authors": [
      {
        "name": "pogyomo",
        "email": "pogyomo@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/pogyomo/tree-sitter-ld65"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
